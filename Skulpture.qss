QMenuBar:selected:!disabled {
    background-color: rgb(100, 200, 40);
    color: rgb(250, 220, 150);
}

QMenu {
    background-color: rgb(100, 200, 150);
    color: rgb(0, 80, 0);
}

QMenu::item:selected:!disabled {
    background-color: rgb(70, 140, 220);
    color: rgb(230, 230, 230);
}

