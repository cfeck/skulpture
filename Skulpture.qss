QProgressBar {
    selection-background-color: rgb(190, 180, 90);
    selection-color: rgb(130, 0, 0);
}

QGroupBox {
    font-weight: bold;
    color: rgba(0, 100, 0, 200);
}

QGroupBox:disabled {
    font-weight: bold;
    color: rgba(0, 100, 0, 80);
}

KTitleWidget > QFrame {
    background-color: rgb(180, 200, 210);
}

